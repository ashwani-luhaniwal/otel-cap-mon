{
    "routes": [
        {
            "source": "^/fesr$",
            "target": "/fesr",
            "destination": "srv_api",
            "csrfProtection": false,
            "authenticationType": "none"
        },
        {
            "source": "/srv/(.*)",
            "destination": "srv_api",
            "authenticationType": "none"
        },
        {
            "source": "/",
            "target": "/",
            "localDir": "webapp",
            "authenticationType": "none"
        }
    ]
}
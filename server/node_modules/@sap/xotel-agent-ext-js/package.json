{
    "name": "@sap/xotel-agent-ext-js",
    "description": "OpenTelemetry Extension",
    "version": "1.5.2",
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "directories": {
        "doc": "docs"
    },
    "scripts": {
        "test": "npm run clean-compile && ts-mocha --timeout 180000 --require mocha-suppress-logs -p tsconfig.json test/*/*.test.ts",
        "test:dev": "ts-mocha --timeout 180000 --require mocha-suppress-logs -p tsconfig.json test/*/*.test.ts",
        "test:coverage": "nyc npm run test",
        "test:coverage-dev": "nyc npm run test:dev",
        "test:pipeline": "npm run test -- --reporter mocha-junit-reporter --reporter-options mochaFile=unit-tests/TEST-junit.xml --exit",
        "test:pipeline-report": "nyc npm run test:pipeline && nyc report --reporter=lcov --reporter=text --reporter=cobertura --report-dir=coverage",
        "compile": "tsc",
        "clean": "rimraf dist/*",
        "clean-compile": "npm run clean && npm run compile"
    },
    "repository": {
        "type": "git",
        "url": "https://github.wdf.sap.corp/xdsr/xotel-agent-ext-js.git"
    },
    "keywords": [
        "OpenTelemetry",
        "Extension"
    ],
    "author": {
        "name": "SAP SE"
    },
    "license": "SAP Developer License Agreement",
    "devDependencies": {
        "@opentelemetry/context-async-hooks": "^1.8.0",
        "@opentelemetry/contrib-test-utils": "0.33.2",
        "@types/chai": "^4.3.4",
        "@types/cfenv": "^1.2.2",
        "@types/mocha": "10.0.1",
        "@types/node": "^18.16.18",
        "@types/node-fetch": "^2.6.4",
        "@types/node-forge": "^1.3.1",
        "@types/sinon": "^10.0.13",
        "@types/uuid": "^9",
        "chai": "^4.3.6",
        "hdb": "0.19.5",
        "mocha-junit-reporter": "^2.2.0",
        "nock": "^13.2.9",
        "mocha-suppress-logs": "0.3.1",
        "nyc": "15.1.0",
        "rimraf": "3.0.2",
        "sinon": "^14.0.0",
        "ts-mocha": "10.0.0",
        "ts-node": "^10.9.1",
        "tslib": "^2.4.0",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "@opentelemetry/api": "1.4.1",
        "@opentelemetry/auto-instrumentations-node": "0.38.0",
        "@opentelemetry/exporter-metrics-otlp-proto": "0.41.2",
        "@opentelemetry/host-metrics": "0.31.0",
        "@opentelemetry/sdk-node": "0.41.2",
        "@sap/xsenv": "^3.1.0",
        "cfenv": "^1.2.4",
        "check-disk-space": "3.4.0",
        "https": "^1.0.0",
        "node-cache": "^5.1.2",
        "node-fetch": "^2.6.9",
        "node-forge": "^1.3.1",
        "uuid": "^9",
        "winston": "^3.10.0"
    },
    "overrides": {
        "semver": "^7.5.2",
        "protobufjs": "^7.2.4"
    }
}